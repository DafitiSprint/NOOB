{
  "manifest_version": 2,

  "name": "Dafiti Alert",
  "description": "This extension recieve notifications from NOOB.",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [ "tabs" ],
  "browser_action": {
    "default_icon": "dft_icon.png",
    "default_popup": "popup.html"
  }
}